{
	"logtype": "weblogic",
	"file": "sample.log",
	"pattern": "(####<)(?P<timestamp>.*?)(>\\s*<)(?P<severity>.*?)(>\\s*<)(?P<subsystem>.*?)(>\\s*<)(?P<machine>.*?)(>\\s*<)(?P<server>.*?)(>\\s*<)(?P<thread>.*?)(>\\s*<)(?P<user>.*?)(>\\s*<)(?P<transaction>.*?)(>\\s*<)(?P<diagcontext>.*?)(>\\s*<)(?P<rawtime>.*?)(>\\s*<)(?P<msgid>.*?)(>\\s*<)(?P<msgtext>.*?)(>)",
	"replacement": {
		"timestamp": {
			"type": "timestamp",
			"format": "MMM dd, yyyy hh:mm:ss a z"
		},
		"severity": {
			"type": "fixed-list",
			"method": "random",
			"list": ["Info", "Warning", "Error"]
		},
		"thread": {
			"type": "integer",
			"method": "increase",
			"min": 0,
			"max": 100
		}
	}

}
