{
  "burstMode": true,
  "concurrency": 1,
  "minInterval": 1,
  "duration": 0,
  "maxEvents": 10,
  "maxInterval": 2,
  "logType": "weblogic",
  "sampleFile": "samples/sample.log",
  "transactionId": [
    "thread",
    "msgid"
  ],
  "maxIntraTransactionLatency": 40,
  "output": {
    "stdout1": {
      "type": "console",
      "attrs": {
        "fileName": "stdout"
      }
    },
    "syslog1": {
      "type": "syslog",
      "attrs": {
        "protocol": "udp",
        "netAddr": "localhost:516",
        "maxBackups": 3,
        "tag": "logspout"
      }
    },
    "file1": {
      "type": "file",
      "attrs": {
        "fileName": "default.log",
        "maxSize": 500,
        "maxBackups": 3,
        "compress": false,
        "maxAge": 7,
        "duplicate": false
      }
    }
  },
  "pattern": [
    "(Start####<)(?<timestamp>.*?)(>\\s*<)(?<severity>.*?)(>\\s*<)(?<subsystem>.*?)(>\\s*<)(?<ipaddress>.*?)(>\\s*<)(?<phone>.*?)(>\\s*<)(?<thread>.*?)(>\\s*<)(?<user>.*?)(>\\s*<)(?<transaction>.*?)(>\\s*<)(?<diagcontext>.*?)(>\\s*<)(?<rawtime>.*?)(>\\s*<BEA-)(?<msgid>.*?)(>\\s*<)(?<msgtext>.*?)(>)",
    "(End####<)(?<timestamp>.*?)(>\\s*<)(?<severity>.*?)(>\\s*<)(?<subsystem>.*?)(>\\s*<)(?<ipaddress>.*?)(>\\s*<)(?<phone>.*?)(>\\s*<)(?<thread>.*?)(>\\s*<)(?<user>.*?)(>\\s*<)(?<transaction>.*?)(>\\s*<)(?<diagcontext>.*?)(>\\s*<)(?<rawtime>.*?)(>\\s*<BEA-)(?<msgid>.*?)(>\\s*<)(?<msgtext>.*?)(>)"
  ],
  "replacement": {
    "timestamp": {
      "type": "timestamp",
      "attrs": {
        "format": "MMM dd, yyyy hh:mm:ss.SSS a z"
      }
    },
    "severity": {
      "type": "fixedList",
      "attrs": {
        "method": "random",
        "listFile": "samples/severity.sample"
      }
    },
    "subsystem": {
      "type": "float",
      "attrs": {
        "min": 100,
        "max": 10000,
        "precision": 20
      }
    },
    "ipaddress": {
      "type": "looksReal",
      "attrs": {
        "method": "ipv4china"
      }
    },
    "phone": {
      "type": "looksReal",
      "attrs": {
        "method": "cellphone-china"
      }
    },
    "user": {
      "type": "fixedList",
      "attrs": {
        "method": "random",
        "list": [
          "GuoJing",
          "HuangRong",
          "ZhangSanfeng",
          "ZhangWuji",
          "LiMochou",
          "OuyangFeng",
          "HongQigong",
          "LinghuChong",
          "RenYingying",
          "DuanTiande"
        ]
      }
    },
    "thread": {
      "type": "integer",
      "attrs": {
        "method": "next",
        "min": 1,
        "max": 100
      }
    },
    "transaction": {
      "type": "string",
      "attrs": {
        "chars": "abcde12345",
        "min": 10,
        "max": 20
      }
    },
    "msgid": {
      "type": "integer",
      "attrs": {
        "method": "random",
        "min": 0,
        "max": 2000000
      }
    }
  }
}
