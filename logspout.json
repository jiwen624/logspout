{
	"concurrency": 10,
	"min-interval": 100,
	"max-interval": 1010,
	"logtype": "weblogic",
	"file": "sample.log",
	"pattern": "(####<)(?P<timestamp>.*?)(>\\s*<)(?P<severity>.*?)(>\\s*<)(?P<subsystem>.*?)(>\\s*<)(?P<machine>.*?)(>\\s*<)(?P<server>.*?)(>\\s*<)(?P<thread>.*?)(>\\s*<)(?P<user>.*?)(>\\s*<)(?P<transaction>.*?)(>\\s*<)(?P<diagcontext>.*?)(>\\s*<)(?P<rawtime>.*?)(>\\s*<BEA-)(?P<msgid>.*?)(>\\s*<)(?P<msgtext>.*?)(>)",
	"replacement": {
		"timestamp": {
			"type": "timestamp",
			"format": "MMM dd, yyyy hh:mm:ss.SSS a z"
		},
		"severity": {
			"type": "fixed-list",
			"method": "random",
			"list": ["Debug", "Info", "Warning", "Error"]
		},
		"subsystem": {
			"type": "fixed-list",
			"method": "random",
			"list": ["Management", "WebLogicServer", "WorkManager", "EJB", "Security"]
		},
		"machine": {
			"type": "fixed-list",
			"method": "random",
			"list": ["machineA", "machineB", "machineC"]
		},
		"server": {
			"type": "looks-real",
			"method": "cellphone-china"
		},
		"user": {
			"type": "fixed-list",
			"method": "next",
			"list": ["GuoJing", "HuangRong", "ZhangSanfeng", "ZhangWuji"]
		},
		"thread": {
			"type": "integer",
			"method": "prev",
			"min": 0,
			"max": 10
		},
		"transaction": {
			"type": "integer",
			"method": "random",
			"min": 0,
			"max": 1000000
		},
		"msgid": {
			"type": "integer",
			"method": "random",
			"min": 0,
			"max": 2000000
		}
	}

}
